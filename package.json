{
  "name": "vria",
  "version": "0.9.3",
  "description": "A Web-based Framework for Creating Immersive Analytics Experiences",
  "author": "PButcher",
  "license": "MIT",
  "repository": "https://github.com/vriajs/vria/",
  "module": "dist/vria.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "bugs": {
    "url": "https://github.com/vriajs/vria/issues"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern",
    "start": "microbundle-crl watch --no-compress --format modern",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd builder && yarn install && yarn run build",
    "deploy": "./node_modules/gh-pages/bin/gh-pages-clean.js && gh-pages -d builder/dist"
  },
  "peerDependencies": {
    "aframe-react": "^4.4.0",
    "prop-types": "^15.7.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "aframe-react": "^4.4.0",
    "ajv": "^6.12.3",
    "d3": "^5.16.0",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "prop-types": "^15.7.2",
    "react-fast-compare": "^3.2.0"
  }
}
